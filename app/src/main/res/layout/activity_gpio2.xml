<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Gpio2Activity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/cl_gpio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp">

        <Switch
            android:id="@+id/sw_gpio8"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_08" />

        <Switch
            android:id="@+id/sw_gpio9"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_09"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio8" />

        <Switch
            android:id="@+id/sw_gpio10"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_10"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio9" />

        <Switch
            android:id="@+id/sw_gpio11"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio11"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio10" />

        <Switch
            android:id="@+id/sw_gpio12"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_12"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio11" />

        <Switch
            android:id="@+id/sw_gpio13"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_13"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio12" />

        <Switch
            android:id="@+id/sw_gpio14"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_14"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio13" />


        <Switch
            android:id="@+id/sw_gpio7"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_07"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio8" />

        <Switch
            android:id="@+id/sw_gpio6"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:enabled="false"
            android:text="NA:_06"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio9"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio7" />

        <Switch
            android:id="@+id/sw_gpio5"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio05"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio10"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio6" />

        <Switch
            android:id="@+id/sw_gpio4"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio04"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio11"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio5" />

        <Switch
            android:id="@+id/sw_gpio3"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio03"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio12"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio4" />

        <Switch
            android:id="@+id/sw_gpio2"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio02"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio13"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio3" />

        <Switch
            android:id="@+id/sw_gpio1"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Gpio01"
            app:layout_constraintLeft_toRightOf="@+id/sw_gpio14"
            app:layout_constraintTop_toBottomOf="@+id/sw_gpio2" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/cl_door2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/cl_gpio">

        <Button
            android:id="@+id/b_hoare2"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Hoare2"
            android:textSize="15dp" />

        <Button
            android:id="@+id/b_lock2"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Lock2"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/b_hoare2" />

        <Button
            android:id="@+id/b_door2"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Door2"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/b_lock2" />

        <Switch
            android:id="@+id/sw_open2"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Open2"
            app:layout_constraintTop_toBottomOf="@+id/b_door2" />

        <Button
            android:id="@+id/btn_GND04"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="GND"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/sw_open2" />

        <Button
            android:id="@+id/btn_GND03"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="GND"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_GND04" />

        <Button
            android:id="@+id/btn_12V02"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="12V"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_GND03" />


    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:id="@+id/cl_door1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toRightOf="@+id/cl_door2"
        app:layout_constraintTop_toBottomOf="@id/cl_gpio">

        <Button
            android:id="@+id/b_hoare1"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Hoare1"
            android:textSize="15dp" />

        <Button
            android:id="@+id/b_lock1"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Lock1"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/b_hoare1" />

        <Button
            android:id="@+id/b_door1"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:padding="0dp"
            android:text="Door1"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/b_lock1" />

        <Switch
            android:id="@+id/sw_open1"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_marginLeft="8dp"
            android:text="Open1"
            app:layout_constraintTop_toBottomOf="@+id/b_door1" />

        <Button
            android:id="@+id/btn_GND02"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="GND"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/sw_open1" />

        <Button
            android:id="@+id/btn_GND01"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="GND"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_GND02" />

        <Button
            android:id="@+id/btn_12V01"
            android:layout_width="85dp"
            android:layout_height="30dp"
            android:enabled="false"
            android:padding="0dp"
            android:text="12V"
            android:textSize="15dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_GND01" />











    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>